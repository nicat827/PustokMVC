@model Slide

<h1 style="text-align:center;">Id: @Model.Id | Title: @Model.Title </h1>
<img src="~/uploads/slide/@Model.ImageURL" style="max-width:100%" />

<hr/>
<h2>Subtitle: <b>@Model.Subtitle</b></h2>
<hr />

<h1>Order: <b>@Model.Order</b></h1>
<hr />


<h1>Description:</h1>
<p>@Model.Description</p>
